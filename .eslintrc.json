{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "plugin:react/recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "single"],
    "react/jsx-uses-react": 2
  }
}
