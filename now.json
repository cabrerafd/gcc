{
  "name": "grace-city",
  "version": 2,
  "alias": "gracecity.now.sh",
  "build": {
    "env": {
      "apiKey": "@gccapikey",
      "authDomain": "@gccauthdomain",
      "databaseURL": "@gccdatabaseurl",
      "projectId": "@gccprojectid",
      "storageBucket": "@gccstoragebucket",
      "messagingSenderId": "@gccmessagingsenderid",
      "appId": "@gccappid"
    }
  },
  "regions": ["sin1"],
  "builds": [{ "src": "package.json", "use": "@now/next@canary" }]
}
